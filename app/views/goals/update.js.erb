<% if @goal.completed? %>
  $("#<%= dom_id(@goal) %>").slideUp();
  $("#completed_goals").append("<%= j render partial: 'completed_goal', object: @goal %>");
<% else %>
  $("#content-wrapper").animate({
    marginBottom: "0in"
  }, 400 );
  $('#edit_goal_form').fadeOut(400);
  $("#<%= dom_id(@goal) %>").remove();
  $('#current_goals').append('<%= j render partial: "current_goal", object: @goal %>');
<% end %>
