<% if @step.completed? %>
  $("#<%= dom_id(@step) %>").remove();
  $("#completed_steps").append("<%= j render partial: 'completed_step', object: @step %>");
<% else %>
  $('#edit_goal_step_form').remove();
  $("#<%= dom_id(@step) %>").html("<%= j render partial: 'current_step', object: @step %>");
<% end %>
